    function swapURL() {
        // make sure a new date was chosen
        if (document.myProd.specdate.options[document.myProd.specdate.selectedIndex].value != "") {
            // all good
            // return true;
            document.myProd.submit();
        } else {
            alert("Please chhose a valid date/time first.");
            return false;
        }
    }
    </script>
